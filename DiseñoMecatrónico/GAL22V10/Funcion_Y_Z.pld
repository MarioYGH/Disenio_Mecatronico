Device   g22v10;

/* Entradas */
PIN 1 = A;
PIN 2 = B;
PIN 3 = C;

/* Salidas */
PIN 19 = Y;   // Nota: Cambia los números de pin según el manual de tu FPGA
PIN 18 = Z;

/* Definición de las funciones lógicas */
Y = !((A # C) $ B);   // Implementa NOT((A OR C) XOR B)
Z = (A & B) # (A # C); // Implementa (A AND B) OR (A OR C)
